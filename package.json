    "sass": "^1.83.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/canvas"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "transform": {
              "react": {
                "runtime": "automatic"
              }
            }
          }
        }
      ]
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid|@solana/web3.js|canvas)/)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.next/",
      "<rootDir>/e2e/"
    ]
  }
}
