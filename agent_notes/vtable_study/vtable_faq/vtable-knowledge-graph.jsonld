{
  "@context": {
    "@vocab": "https://visactor.io/vtable/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "hasFeature": "rdfs:hasFeature",
    "relatedTo": "rdfs:relatedTo",
    "implements": "rdfs:implements",
    "requires": "rdfs:requires",
    "partOf": "rdfs:partOf"
  },
  "@graph": [
    {
      "@id": "VTable",
      "@type": "Component",
      "rdfs:label": "VTable Component",
      "rdfs:description": "A high-performance table component for data visualization",
      "hasFeature": [
        "BasicTable",
        "PivotTable",
        "TreeTable",
        "EditableTable"
      ]
    },
    {
      "@id": "BasicTable",
      "@type": "TableType",
      "rdfs:label": "Basic Table Features",
      "implements": [
        "HeaderManagement",
        "CellManagement",
        "StyleManagement",
        "EventHandling"
      ]
    },
    {
      "@id": "HeaderManagement",
      "@type": "Feature",
      "rdfs:label": "Header Management",
      "hasFeature": [
        "MultiLevelHeaders",
        "HeaderStyling",
        "HeaderMerging"
      ]
    },
    {
      "@id": "CellManagement",
      "@type": "Feature",
      "rdfs:label": "Cell Management",
      "hasFeature": [
        "CellEditing",
        "CellMerging",
        "CustomRendering",
        "CellSelection"
      ]
    },
    {
      "@id": "StyleManagement",
      "@type": "Feature",
      "rdfs:label": "Style Management",
      "hasFeature": [
        "TextStyling",
        "BorderStyling",
        "ColorGradients",
        "Highlighting"
      ]
    },
    {
      "@id": "EventHandling",
      "@type": "Feature",
      "rdfs:label": "Event Handling",
      "hasFeature": [
        "MouseEvents",
        "KeyboardEvents",
        "SelectionEvents",
        "EditEvents"
      ]
    },
    {
      "@id": "PivotTable",
      "@type": "TableType",
      "rdfs:label": "Pivot Table Features",
      "implements": [
        "DimensionManagement",
        "Aggregation",
        "DrillDown",
        "ProgressiveLoading"
      ]
    },
    {
      "@id": "TreeTable",
      "@type": "TableType",
      "rdfs:label": "Tree Table Features",
      "implements": [
        "NodeManagement",
        "TreeExpansion",
        "LazyLoading",
        "NodeStyling"
      ]
    },
    {
      "@id": "EditableTable",
      "@type": "TableType",
      "rdfs:label": "Editable Table Features",
      "implements": [
        "CellEditing",
        "ValidationRules",
        "EditMode",
        "UndoRedo"
      ]
    },
    {
      "@id": "VisualFeatures",
      "@type": "Category",
      "rdfs:label": "Visual Features",
      "hasFeature": [
        "Sparklines",
        "ProgressBars",
        "Heatmaps",
        "MiniCharts",
        "CustomIcons"
      ]
    },
    {
      "@id": "InteractiveFeatures",
      "@type": "Category",
      "rdfs:label": "Interactive Features",
      "hasFeature": [
        "DragAndDrop",
        "Tooltips",
        "ContextMenus",
        "HoverEffects",
        "ScrollBehavior"
      ]
    },
    {
      "@id": "DataManagement",
      "@type": "Category",
      "rdfs:label": "Data Management",
      "hasFeature": [
        "Sorting",
        "Filtering",
        "Aggregation",
        "DataValidation",
        "StateManagement"
      ]
    },
    {
      "@id": "CustomRendering",
      "@type": "Feature",
      "rdfs:label": "Custom Rendering",
      "implements": [
        "HTMLRendering",
        "CanvasRendering",
        "CustomLayout",
        "IconRendering"
      ]
    },
    {
      "@id": "Performance",
      "@type": "Category",
      "rdfs:label": "Performance Features",
      "hasFeature": [
        "VirtualScrolling",
        "LazyLoading",
        "BatchProcessing",
        "CacheManagement"
      ]
    },
    {
      "@id": "Integration",
      "@type": "Category",
      "rdfs:label": "Integration Features",
      "hasFeature": [
        "VChartIntegration",
        "VueIntegration",
        "CustomComponents",
        "ExternalEvents"
      ]
    }
  ]
}