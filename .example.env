# Note: You either need BING_SEARCH_API or SERPER_API_KEY
TOGETHER_API_KEY=
BING_API_KEY=
SERPER_API_KEY=
HELICONE_API_KEY=
FLIPSIDE_API_KEY=

# OpenSVM RPC endpoints
OPENSVM_RPC_LIST=["263c9f53f4e4cdb897c0edc4a64cd007"]
OPENSVM_RPC_LIST_2=[]

# Qdrant Database Configuration
QDRANT=your_qdrant_api_key_here
QDRANT_SERVER=https://your-qdrant-instance.qdrant.tech
# Debug and logging configuration
DEBUG=false
LOG_LEVEL=error

# OpenRouter Configuration (for Anthropic API Mirror)
OPENROUTER_API_KEY=your_openrouter_key_here
OPENROUTER_API_KEYS=key1,key2,key3  # Comma-separated for multiple keys

# Anthropic API Mirror - Timeout Configuration (milliseconds)
ANTHROPIC_REQUEST_TIMEOUT=30000
ANTHROPIC_STREAMING_TIMEOUT=120000
ANTHROPIC_CONNECTION_TIMEOUT=10000
ANTHROPIC_RETRY_ATTEMPTS=3
ANTHROPIC_RETRY_DELAY=1000

# Anthropic API Mirror - Database Configuration (optional - for persistence)
DATABASE_URL=postgresql://user:password@localhost:5432/opensvm
ENABLE_DATABASE_PERSISTENCE=false

# Anthropic API Mirror - Health Check Configuration
HEALTH_CHECK_INTERVAL=60000  # Check every 60s
ENABLE_HEALTH_CHECKS=true
